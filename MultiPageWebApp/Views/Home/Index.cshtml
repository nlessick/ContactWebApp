@model List<MultiPageWebApp.Models.Contact>
@{
    ViewBag.Title = "My Contacts";
}

<h2>Movie List</h2>
<a asp-controller="Contact" asp-action="Add">Add New Contact</a>
<table class="table table-bordered table-striped">
    <thead>
        <tr><th>Name</th><th>Year</th><th>Genre</th><th>Rating</th><th></th></tr>
    </thead>
    <tbody>
        @foreach (var contact in Model)
        {
            <tr>
                <td>@contact.Name</td>
                <td>@contact.PhoneNumber</td>
                <td>@contact.Address</td>
                <td>@contact.Note</td>
                <td>
                    <a asp-controller="Movie" asp-action="Edit"
                       asp-route-id="@contact.PhoneId"
                       asp-route-slug="@contact.Slug">Edit</a>
                    <a asp-controller="Movie" asp-action="Delete"
                       asp-route-id="@contact.PhoneId"
                       asp-route-slug="@contact.Slug">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>